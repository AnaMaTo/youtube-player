{
  "plugins": [
    "transform-object-rest-spread",
    "transform-flow-strip-types",
    "transform-runtime",
    "add-module-exports"
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        },
        "useBuiltIns": true
      }
    ]
  ]
}
